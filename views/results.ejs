  <script>
    var page = 1;
    var userSelections = [];
    var pass;
    var buttons = document.querySelectorAll(".btnSelection");
    var pages = document.querySelectorAll(".page");
    var back = document.querySelectorAll(".backBtn");

    for (i = 0; i < buttons.length; i++) {
      buttons[i].addEventListener("click", function() {
        // on button click, add data-eval value to array and send to next page function
        userSelections.push(this.dataset.eval);
        nextPage();
      });
    }

    function nextPage() {
      if (page < pages.length) {
        // only go to last page
        page++;
        //update progress bar
        progressBar();
      }
      for (i = 0; i < pages.length; i++) {
        pages[i].classList.remove("active");
      }
      document.querySelector(".page" + page).classList.add("active");
      if (page == pages.length){
        lastPage();
      }
      console.log(userSelections);
    }

    function lastPage() {
      // see if user passes or fails
      // these are checking for correct answers
      if(userSelections.includes("q1-1") && userSelections.includes("q2-2") && userSelections.includes("q4-2") && userSelections.includes("q5-2") && userSelections.includes("q6-1") ) {
        pass = true;
      }
      else {
        pass = false;
      }
      console.log(pass);
    }

    function progressBar() {
      var math = (page / (pages.length)) * 100;
      document.querySelector(".barSpan").style.width = math + "%";
    }

    for (i = 0; i < back.length; i++) {
      back[i].addEventListener("click", function() {
        prevPage();
      });
    }

    function prevPage() {
      page--;
      //update progress bar
      progressBar();

      for (i = 0; i < pages.length; i++) {
        pages[i].classList.remove("active");
      }
      document.querySelector(".page" + page).classList.add("active");
      
      //remove last added to array
      userSelections.pop();
      
      console.log(userSelections);
    }
  </script>
    
